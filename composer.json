{
    "name": "chrisdunko/mongo-php",
    "description": "A PHP system that uses MongoDB.",
    "version": "0.1.0",
    "type": "project",
    "keywords": ["PHP", "MongoDB"],
    "license": "do-whatever-but-do-not-blame-me",
    "authors": [
        {
            "name": "Christoph Dunkake",
            "email": "christoph@dragoneyes.software",
            "homepage": "https://dragoneyes.software",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "mongodb/mongodb": "^1.16",
        "ext-mongo": "*"
    },
    "require-dev": {},
    "autoload": {
        "psr-4": {
            "Controllers\\": "controllers/",
            "Models\\": "models/",
            "Enums\\": "enums/",
            "Helpers\\": "helpers/"
        },
        "files": [
            "Router.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
        }
    },
    "scripts": {},
    "extra": {
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
