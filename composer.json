{
    "name": "dragon-eyes/user-group-manager-v4",
    "description": "A PHP system that uses MongoDB.",
    "version": "0.1.0",
    "type": "project",
    "keywords": ["PHP", "MongoDB"],
    "license": "proprietary",
    "authors": [
        {
            "name": "Christoph Dunkake",
            "email": "christoph@dragoneyes.software",
            "homepage": "https://dragoneyes.software",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "mongodb/mongodb": "^1.16"
    },
    "require-dev": {
        "phpunit/phpunit": "^10"
    },
    "autoload": {
        "psr-4": {
            "Controllers\\": "controllers/",
            "Models\\": "models/",
            "Enums\\": "enums/",
            "Helpers\\": "helpers/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Logs\\": "modules/logs/",
            "Users\\": "modules/users/",
            "Tokens\\": "modules/tokens/"
        },
        "files": [
            "Router.php",
            "modules/core/controllers/Template_Controller.php",
            "modules/core/models/Core_Model.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {},
    "extra": {
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
